{
  "version": 2,
  "builds": [
    {
      "src": "src/*",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.njk",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
